SELECT column1, column2, column3
FROM table_name;

MySql
SELECT id, name, email, balance, status
INTO OUTFILE 'C:/Users/YourUser/Desktop/user.csv'
FIELDS TERMINATED BY ','
ENCLOSED BY '"'
LINES TERMINATED BY '\n'
FROM user;

Include header
SELECT 'column1','column2','column3'
UNION ALL
SELECT column1, column2, column3
FROM table_name
INTO OUTFILE 'C:/path/to/file.csv'
FIELDS TERMINATED BY ','
ENCLOSED BY '"'
LINES TERMINATED BY '\n';
